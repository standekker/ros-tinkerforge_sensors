<launch>
  <node name="tfsensors" pkg="tinkerforge_sensors" type="tinkerforge_sensors_node" output="screen">
    <rosparam param="UID_to_Topic">
      6wViuNa : 'imu_v2'
    </rosparam>
    <rosparam>
      linear_acceleration_stddev : 0.1
      angular_velocity_stddev : 0.1
      magnetic_field_stddev : 0.1
      orientation_stddev : 0.1
      frame_id : "imu_link"
    </rosparam>
  </node>
</launch>
